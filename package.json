{
  "name": "game-client",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "test": "nyc ./node_modules/.bin/_mocha 'test/**/*Test.ts'",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "verify": "npm run test && npm run lint",
    "clean": "node ./build/clean.js",
    "optimization:art": "node ./build/art-optimization.mjs",
    "blure": "node ./build/blure.js",
    "make-atlases": "node ./build/assets.js",
    "make-asprite": "node ./build/make-asprite.js",
    "build:sounds": "npm run make-asprite && npm run move-assets-to-dist",
    "move-assets-to-dist": "node ./build/build.js",
    "build-assets": "npm run clean && npm run make-asprite && npm run make-atlases && npm run move-assets-to-dist",
    "build:webpack": "webpack --config ./webpack.config.js",
    "build": "npm run verify && npm run move-assets-to-dist && npm run build:webpack",
    "build:prod": "env-cmd npm run verify && npm run build-assets && env-cmd npm run build:webpack",
    "build:dev": "env-cmd webpack --config ./webpack.config.js --mode development",
    "teamcity:build:dev": "webpack --config ./webpack.config.js --mode development",
    "teamcity:build:prod": "webpack --config ./webpack.config.js",
    "build:dev:all": "npm run build-assets && env-cmd webpack --config ./webpack.config.js --mode development",
    "serve": "env-cmd webpack-dev-server --mode development",
    "prod-serve": "env-cmd webpack-dev-server",
    "http:server": "http-server ./dist -p 80"
  },
  "keywords": [],
  "author": "hlobka",
  "license": "ISC",
  "dependencies": {
    "@pixi/particle-emitter": "^5.0.8",
    "dat.gui": "0.7.8",
    "gsap": "3.9.0",
    "howler": "2.2.0",
    "pixi-spine": "^4.0.4",
    "pixi.js": "^7.3.2",
    "stats.js": "0.17.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@pixi-spine/runtime-4.0": "^4.0.3",
    "@pixi/assets": "^7.3.2",
    "@pixi/events": "^7.3.2",
    "@pixi/filter-color-matrix": "^7.3.2",
    "@pixi/filter-glow": "^5.2.1",
    "@pixi/graphics": "^7.3.2",
    "@pixi/text": "^7.3.2",
    "@pixi/text-bitmap": "^7.3.2",
    "@testdeck/mocha": "0.2.0",
    "@types/chai": "4.3.0",
    "@types/dat.gui": "0.7.7",
    "@types/gsap": "3.0.0",
    "@types/howler": "2.2.4",
    "@types/node": "17.0.5",
    "@types/sinon": "10.0.6",
    "@types/stats.js": "0.17.0",
    "@types/webpack-env": "1.16.0",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "audiosprite": "0.7.2",
    "babel-loader": "8.2.3",
    "chai": "4.3.4",
    "copy-webpack-plugin": "10.0.0",
    "css-loader": "6.5.1",
    "env-cmd": "^10.1.0",
    "eslint": "8.22.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-import": "2.27.5",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "http-server": "^13.0.1",
    "imagemin": "^8.0.1",
    "imagemin-pngquant": "^9.0.2",
    "mocha": "10.1.0",
    "mocha-typescript": "1.1.17",
    "nyc": "15.1.0",
    "raw-loader": "4.0.2",
    "sinon": "12.0.1",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.2.5",
    "texturepackerify": "0.0.14",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tslint": "6.1.3",
    "typescript": "4.5.2",
    "webpack": "5.76.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.6.0"
  }
}
